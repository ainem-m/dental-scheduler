{
  "name": "dental-scheduler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "ts-node server/src/index.ts",
    "dev": "vite",
    "build": "vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "preview": "vite preview",
    "test:client": "vitest run -c vitest.config.js",
    "test:server": "vitest run -c vitest.config.server.js server/tests",
    "test": "npm run test:client && npm run build:server && npm run test:server",
    "test:docker": "docker compose -f docker-compose.yml -f docker-compose.test.yml run --rm app npm test",
    "test:e2e": "playwright test",
    "migrate": "knex migrate:latest",
    "db:check": "knex migrate:list",
    "seed": "knex seed:run",
    "dev:concurrently": "concurrently \"npm:start\" \"npm:dev\""
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "basic-auth": "^2.0.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.0.0",
    "express": "^4.19.2",
    "fs-extra": "^11.3.0",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "pixelmatch": "^7.1.0",
    "socket.io": "^4.7.5",
    "uuid": "^11.1.0",
    "vite": "^5.2.8",
    "vue": "^3.4.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@types/basic-auth": "^1.1.8",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.14",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^8.2.2",
    "jsdom": "^24.0.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^1.6.0"
  }
}
