{
  "name": "dental-scheduler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server/src/index.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:client": "vitest run -c vitest.config.js",
    "test:server": "vitest run -c vitest.config.server.js",
    "test": "npm run test:client && npm run test:server",
    "test:docker": "docker compose -f docker-compose.yml -f docker-compose.test.yml run --rm app npm test",
    "test:e2e": "playwright test",
    "migrate": "knex migrate:latest",
    "db:check": "knex migrate:list",
    "seed": "knex seed:run",
    "dev:concurrently": "concurrently \"npm:start\" \"npm:dev\""
  },
  "dependencies": {
    "basic-auth": "^2.0.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^11.0.0",
    "express": "^4.19.2",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.5",
    "uuid": "^11.1.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^8.2.2",
    "jsdom": "^24.0.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.0.0",
    "vite": "^5.2.8",
    "vitest": "^1.6.0"
  }
}

